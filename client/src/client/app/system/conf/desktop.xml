<?xml version="1.0" encoding="utf-8"?>
<desktop>
    
	<title>Haberdashery Management</title>
	<icon>icon/icon.png</icon>
	<laf>com.sun.java.swing.plaf.windows.WindowsLookAndFeel</laf>
	<bgcolor>230</bgcolor>
	<login>system.tasks.Login</login>
	<exit>system.tasks.Exit</exit>
	<blocker_time>10</blocker_time> <!-- in minutes -->
	
	<shortcut>
		<name>Haberdashery Management</name>
		<icone>icon/icon.ico</icone>
		<base_url>http://SERVER_URL/</base_url>
		<file_url>/application.jnlp</file_url>
	</shortcut>
	
	<messages>
		<information>Information</information>
		<confirm>Confirmation</confirm>
		<warning>Warning</warning>
		<error>Error</error>
		<no_permission>The user does not have permissions to perform this operation</no_permission>
		<communication_error>Communication error</communication_error>
		<wait>Please, wait...</wait>
		<true_string>Yes</true_string>
		<false_string>No</false_string>
	</messages>
	
</desktop>